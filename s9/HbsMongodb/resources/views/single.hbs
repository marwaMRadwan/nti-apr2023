{{>header}}
{{>nav}}
<section class="my-5 container">
    {{#if user}}
    <div>
        <h3>{{user.id}}</h3>
        <h3>{{user.name}}</h3>
        <h3>{{user.email}}</h3>
        <h3>{{user.age}}</h3>
    </div>
    {{else}}
    <div class="alert alert-danger">
        No user found
    </div>
    {{/if}}
</section>
{{>footer}}